public class Solution {
    public int[] TwoSum(int[] nums, int target) {
        int[] ans=new int[2];
        for(int i=0;i<=nums.Length-1;i++){
            for(int n=i+1;n<=nums.Length-1;n++){
                if(nums[i]+nums[n]==target){
                    ans[0]=i;
                    ans[1]=n;
                    return ans;
                }
            }
        }
        
        return null;
    }
}
